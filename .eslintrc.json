{
  "extends": ["next/core-web-vitals", "next", "prettier"],
  "rules": {
    "react/no-unescaped-entities": "off",
    "@next/next/no-img-element": "error",
    "react/prop-types": "off",
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "prefer-const": "error",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
  }
}
